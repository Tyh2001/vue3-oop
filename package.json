{
  "name": "vue3-oop",
  "version": "1.0.0",
  "main": "dist/vue3-oop.js",
  "module": "dist/vue3-oop.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-oop.mjs",
      "require": "./dist/vue3-oop.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "type:check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "release": "pnpm build && release-it",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "postinstall": "simple-git-hooks"
  },
  "files": [
    "dist",
    "types",
    "package.json"
  ],
  "author": {
    "name": "agileago",
    "email": "593728759@qq.com"
  },
  "keywords": [
    "vue-oop",
    "oop",
    "vue",
    "di",
    "vue-di",
    "ioc",
    "vue class component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agileago/vue3-oop.git"
  },
  "homepage": "https://agileago.github.io/vue3-oop",
  "license": "MIT",
  "devDependencies": {
    "@abraham/reflection": "^0.10.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@release-it/conventional-changelog": "^5.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.7.16",
    "@types/prettier": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.23.2",
    "@vitest/ui": "^0.23.2",
    "@vue/test-utils": "2.0.2",
    "@vue3-oop/plugin-vue-jsx": "^1.4.2",
    "ant-design-vue": "^3.2.12",
    "autobind-decorator": "^2.4.0",
    "c8": "^7.12.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^6.0.4",
    "injection-js": "^2.4.0",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "release-it": "^15.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.9",
    "simple-git-hooks": "^2.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.5.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitepress": "1.0.0-alpha.13",
    "vitest": "^0.23.2",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json,js}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "peerDependencies": {
    "injection-js": "*",
    "vue": "3"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "ignoreRecommendedBump": true
      }
    },
    "github": {
      "release": true,
      "web": true
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  }
}
